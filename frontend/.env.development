# Frontend Development Environment Variables

# API URL - Use localhost for development
VITE_API_URL=http://localhost:8000

# OAuth Configuration
VITE_OAUTH_CLIENT_ID=onestep-frontend-client
VITE_OAUTH_CLIENT_SECRET=onestep-frontend-secret-dev

# App Configuration
VITE_APP_TITLE=OneStep - Development
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_API=false
