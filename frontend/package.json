{
  "name": "onestep-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "concurrently \"npm run mock-api\" \"npm run dev\"",
    "mock-api": "node mock-api/auth-server.cjs",
    "mock-api:simple": "json-server --watch mock-api/db.json --port 8000 --routes mock-api/routes.json --middlewares mock-api/middleware.js",
    "build": "vite build",
    "build:check": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "test:unit": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@mdi/font": "^7.4.0",
    "@tanstack/vue-query": "^5.17.0",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "date-fns": "^3.0.0",
    "pinia": "^2.1.0",
    "vee-validate": "^4.12.0",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.0",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.0",
    "vuetify": "^3.4.0",
    "yup": "^1.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-vue": "^9.20.0",
    "json-server": "^0.17.4",
    "jsonwebtoken": "^9.0.2",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite-plugin-vuetify": "^2.0.0",
    "vitest": "^1.2.0",
    "vue-tsc": "^1.8.0"
  }
}
